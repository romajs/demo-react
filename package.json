{
  "name": "demo-react",
  "version": "0.1.0",
  "private": true,
  "author": "@romajs",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pre-commit": [
    "lint",
    "test:ci"
  ],
  "proxy": "http://localhost:3001",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint:watch": "npm run lint -- --watch",
    "lint": "esw src --color --fix",
    "start:app": "react-scripts start",
    "start:mock": "json-server mock/db.json",
    "start": "run-p start:app start:mock",
    "test:ci": "cross-env CI=true npm test",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-router-prop-types": "^1.0.4",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "remarkable": "^1.7.1",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-config-standart": "^0.2.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-watch": "^5.0.1",
    "json-server": "^0.14.2",
    "pre-commit": "^1.2.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3"
  }
}
